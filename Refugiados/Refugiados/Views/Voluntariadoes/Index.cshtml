@model IEnumerable<Refugiados.Models.Voluntariado>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CPF)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Conselho)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Uf_Conselho)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Numero_Conselho)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Especialidade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Servicos)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CPF)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Conselho)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Uf_Conselho)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Numero_Conselho)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Especialidade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Servicos)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_Voluntario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_Voluntario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_Voluntario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
